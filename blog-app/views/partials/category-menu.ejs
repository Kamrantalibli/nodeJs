<div class="list-group">
    <a href="/blogs" class="list-group-item list-group-item-action">All categories</a>
    <% categories.forEach((category)=>{ %>
        <a href="/blogs/category/<%= category.url %>" class="list-group-item list-group-item-action <%= selectedcategory == category.url ? 'active' : '' %>">    
            <%= category.name %>
        </a>
    <% }) %>
</div>